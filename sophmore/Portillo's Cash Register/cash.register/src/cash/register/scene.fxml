<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="200" prefWidth="320" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cash.register.RegisterController">
    <children>
        <Button fx:id="button" layoutX="40.0" layoutY="88.0" onAction="#handlePay" prefHeight="8.0" prefWidth="35.0" text="Pay" visible="false" />
      <Button layoutX="175.0" layoutY="22.0" mnemonicParsing="false" onAction="#handleBeef" text="Beef" />
      <Button layoutX="102.0" layoutY="22.0" mnemonicParsing="false" onAction="#handleShake" text="Shake" />
      <Button layoutX="249.0" layoutY="22.0" mnemonicParsing="false" onAction="#handleFries" text="Fries" />
      <Button layoutX="23.0" layoutY="22.0" mnemonicParsing="false" onAction="#handleBurger" text="Burger" />
        <Label fx:id="lblTotal" contentDisplay="CENTER" layoutX="37.0" layoutY="62.0" minHeight="16" minWidth="69" prefHeight="17.0" prefWidth="247.0" text="Total:" />
      <Label fx:id="lblChange" alignment="TOP_LEFT" layoutX="37.0" layoutY="122.0" prefHeight="52.0" prefWidth="261.0" />
      <TextField fx:id="cashInput" layoutX="111.0" layoutY="88.0" prefWidth="80.0" promptText="00.00" />
      <Label layoutX="102.0" layoutY="88.0" prefHeight="25.0" prefWidth="9.0" text="\$" />
    </children>
</AnchorPane>
